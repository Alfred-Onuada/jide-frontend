{
  "expo": {
    "scheme": "com.jide.me",
    "name": "SmartDoc",
    "slug": "jide-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Group.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Group.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.m.i.jidefrontend"
    },
    "web": {
      "favicon": "./assets/Group.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "95482368-4d5a-4865-bbaf-cefabf19d3c0"
      }
    }
  }
}
